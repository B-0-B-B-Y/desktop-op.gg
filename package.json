{
  "name": "desktop-op.gg",
  "version": "1.0.0",
  "author": "Bob Naydenov <bob.naydenov@hotmail.com>",
  "description": "An elegant desktop version of OP.GG.",
  "license": "GPL-3.0",
  "keywords": [
    "desktop",
    "app",
    "opgg",
    "league of legends",
    "league",
    "of",
    "legends",
    "statistics"
  ],
  "main": "./main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/B-0-B-B-Y/desktop-op.gg.git"
  },
  "build": {
    "appId": "desktop.op.gg",
    "productName": "OP.GG Desktop",
    "mac": {
      "category": "Social Networking",
      "icon": "app/build/icon.icns"
    },
    "win": {
      "target": "NSIS",
      "icon": "app/build/icon.ico"
    },
    "linux": {
      "icon": "app/build/"
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder -mlw",
    "dist-win": "electron-builder --win",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "electron": "^1.7.12",
    "electron-ad-blocker": "^0.2.0",
    "electron-builder": "^19.56.2",
    "electron-prebuilt": "^1.4.13",
    "electron-rebuild": "^1.7.3"
  },
  "dependencies": {}
}
